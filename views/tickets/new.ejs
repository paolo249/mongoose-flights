<%- include('../partials/header') %>

<form id="add-ticket-form" action="/flights/<%=flightid%>/tickets" method="POST">
  <label>Seat:</label>
  <input name="seat" required pattern="[A-F][1-9]\d?"> <br>
  <label>Price:</label>
  <input name="price">
  <input type="submit" value="Add Ticket">
</form>

